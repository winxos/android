<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".main">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="#ffd6ffe9">

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/editText3"
            android:text="10001"
            android:editable="false"
            android:enabled="false" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="broadcast"
            android:id="@+id/button2"
            android:onClick="broadcast" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="listen"
            android:id="@+id/button"
            android:onClick="listen" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/editText"
            android:text="hello world" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="sendto"
            android:id="@+id/button1"
            android:onClick="sendto" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="viewlogs"
            android:id="@+id/button3"
            android:onClick="viewlogs" />

        <ListView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/listView"
            android:layout_gravity="center_horizontal"
            tools:listitem="@android:layout/simple_list_item_multiple_choice"
            android:choiceMode="multipleChoice" />

    </LinearLayout>
</RelativeLayout>
